{
    "name": "safiull/laravel-installer",
    "description": "Laravel website installer package",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Safiull Alam",
            "email": "safiullalam9931@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0"
    },
    "autoload": {
        "psr-4": {
            "Safiull\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Safiull\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}